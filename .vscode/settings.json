{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++17 src/*.cpp -Iinclude -o bin/$fileNameWithoutExt && ./bin/$fileNameWithoutExt"
    },
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/include"
    ],
    "files.associations": {
        "iostream": "cpp"
    }
}